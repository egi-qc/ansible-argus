---
# PAP 
pap_host: example.org
pap_port: 8150
pap_host_dn: /DC=foo/DC=bar/O=baz/CN=host/example.org
pap_admin_dn:
  - /DC=foo/DC=bar/O=baz/CN=user-name
pap_port_shutdown: 8150
pap_host_cert: /etc/grid-security/hostcert.pem
pap_host_key: /etc/grid-security/hostkey.pem
# PDP
pdp_host: example.org
pdp_port: 8152
pdp_port_admin: 8153 
pdp_pass_admin: pdpadmin_pass
pdp_host_cert: /etc/grid-security/hostcert.pem
pdp_host_key: /etc/grid-security/hostkey.pem
# PEPD
pepd_host: example.org
pepd_port: 8154
pepd_port_admin: 8155
pepd_pass_admin: 8155
pepd_host_cert: /etc/grid-security/hostcert.pem
pepd_host_key: /etc/grid-security/hostkey.pem
